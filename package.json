{
  "name": "pets-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "SET NODE_ENV=development & npm-run-all server",
    "build": "babel ./src --out-dir dist",
    "prod": "SET NODE_ENV=production & npm-run-all build server:prod",
    "watch": "nodemon"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.4.2",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5"
  }
}
